<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- 진단 설정 -->
	<package name="faultSetting" namespace="/faultSetting" extends="json-hierarchy">
		<action name="loadFaultSettingContent" method="loadFaultSettingContent" class="faultSettingAction">
			<result type="freemarker">/WEB-INF/pages/analysis/diagnosis/diagnosisSetting/listSettingContent.ftl</result>
		</action>	
		<action name="loadFaultTableInListContent" method="loadFaultTableInListContent" class="faultSettingAction">
			<result type="freemarker">/WEB-INF/pages/analysis/diagnosis/diagnosisSetting/settingTableInListContent.ftl</result>
		</action>
		
		<!-- 진단 -->
		<action name="startFaultCheck" method="startFaultCheck" class="faultSettingAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 템플릿 저장 -->
		<action name="saveFaultCheckTemplate" method="saveFaultCheckTemplate" class="faultSettingAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 템플릿 삭제 -->
		<action name="delFaultCheckTemplate" method="delFaultCheckTemplate" class="faultSettingAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 예약 -->
		<action name="registerFaultCheckSchedule" method="registerFaultCheckSchedule" class="faultSettingAction">
			<result type="jsonHierarchy"/>
		</action>
		
	</package>
	
	<!-- 진단 진행 (진단중 팝업) -->
	<package name="faultDiagnosis" namespace="/faultDiagnosis" extends="json-hierarchy">
		<action name="loadDiagnosisStateInfo" method="loadDiagnosisStateInfo" class="faultDiagnosisAction">
			<result type="jsonHierarchy"/>
		</action>
		<action name="loadDiagnosisChartData" method="loadDiagnosisChartData" class="faultDiagnosisAction">
			<result type="jsonHierarchy"/>
		</action>
		<action name="cancelFaultCheck" method="cancelFaultCheck" class="faultDiagnosisAction">
			<result type="jsonHierarchy"/>
		</action>	
	</package>
	
	<!-- 진단 이력 -->
	<package name="faultHistory" namespace="/faultHistory" extends="json-hierarchy">
		<action name="retrieveFaultHistoryTotal" method="retrieveFaultHistoryTotal" class="faultHistoryAction">
			<result type="jsonHierarchy"/>
		</action>	
		<action name="loadFaultListContent" method="loadFaultListContent" class="faultHistoryAction">
			<result type="freemarker">/WEB-INF/pages/analysis/diagnosis/diagnosisHistory/listHistoryContent.ftl</result>
		</action>	
		<action name="loadHistoryTableInListContent" method="loadFaultListContent" class="faultHistoryAction">
			<result type="freemarker">/WEB-INF/pages/analysis/diagnosis/diagnosisHistory/historyTableInListContent.ftl</result>
		</action>
		
		<action name="loadFaultResult" method="loadFaultResult" class="faultHistoryAction">
			<result type="freemarker">/WEB-INF/pages/analysis/diagnosis/diagnosisHistory/resultContent.ftl</result>
		</action>
		
		<!-- 진단 History 삭제 -->
		<action name="delHistorys" method="delHistorys" class="faultHistoryAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 진단 결과 Chart 정보 -->
		<action name="loadCpuChartInfo" method="loadCpuChartInfo" class="faultHistoryAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 진단 결과 Progressbar 정보 -->
		<action name="loadProgressbarInfo" method="loadProgressbarInfo" class="faultHistoryAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 진단 결과 PacketLoss 정보 -->
		<action name="loadPacketLossInfo" method="loadPacketLossInfo" class="faultHistoryAction">
			<result type="jsonHierarchy"/>
		</action>
				
		<!-- 진단이력에 예약정보 -->
		<action name="loadScheduleInfo" method="loadScheduleInfo" class="faultHistoryAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 예약 삭제 -->
		<action name="deleteFaultScheduleInfo" method="deleteFaultScheduleInfo" class="faultHistoryAction">
			<result type="jsonHierarchy"/>
		</action>
	
		<!-- 패킷 덤프 파일 유무 검사 -->
		<action name="checkSvcPktDumpDataExist" method="checkSvcPktDumpDataExist" class="faultHistoryAction">
			<result type="json">
				<param name="ignoreHierarchy">false</param>
			</result>
		</action>
		
		<!--  패킷 덤프 다운로드 -->
		<action name="downloadSvcPktDump" method="downloadSvcPktDump" class="faultHistoryAction">
        	<result type="stream">
                <param name="contentType">application/octet-stream</param>
                <param name="contentLength">${contentLength}</param>
                <param name="contentDisposition">${contentDisposition}</param>
                <param name="inputName">inputStream</param>
                <param name="bufferSize">8192</param>
            </result>
        </action>
		
		<!-- 진단 결과 내보내기 -->
		<action name="checkResultDataExist" method="checkResultDataExist" class="faultHistoryAction">
			<result type="json">
				<param name="ignoreHierarchy">false</param>
			</result>
		</action>	
			
		<action name="downloadResultData" method="downloadResultData" class="faultHistoryAction">
        	<result type="stream">
                <param name="contentType">application/octet-stream</param>
                <param name="contentLength">${contentLength}</param>
                <param name="contentDisposition">${contentDisposition}</param>
                <param name="inputName">inputStream</param>
                <param name="bufferSize">8192</param>
            </result>
        </action>
	</package>	
	
	<!-- 진단 패킷분석 -->
	<package name="faultAnalysis" namespace="/faultAnalysis" extends="json-hierarchy">
		
		<!-- 패킷 목록 TotalCount -->
		<action name="retrieveFaultAnalysisTotal" method="retrieveFaultAnalysisTotal" class="faultAnalysisAction">
			<result type="jsonHierarchy"/>
		</action>	
		
		<!-- 패킷분석 리스트 -->
		<action name="loadFaultAnalysisListContent" method="loadFaultAnalysisListContent" class="faultAnalysisAction">
			<result type="freemarker">/WEB-INF/pages/analysis/packets/listAnalysisContent.ftl</result>
		</action>	
		<action name="loadPktDumpStatusInfoContent" method="loadPktDumpStatusInfoContent" class="faultAnalysisAction">
			<result type="jsonHierarchy" />
		</action>
		
		<!-- 패킷분석 port정보 -->
		<action name="loadFaultAnalysisPortContent" method="loadFaultAnalysisPortContent" class="faultAnalysisAction">
			<result type="freemarker">/WEB-INF/pages/analysis/packets/pktDumpPortTableInListContent.ftl</result>
		</action>
		
		<!-- 패킷분석 port정보 -->		
		<action name="loadFaultAnalysisPort" method="loadFaultAnalysisPort" class="faultAnalysisAction">
			<result type="jsonHierarchy" />
		 </action>		
		
		<!-- 패킷분석 paging 데이터 -->
		<action name="loadAnalysisTableInListContent" method="loadFaultAnalysisListContent" class="faultAnalysisAction">
			<result type="freemarker">/WEB-INF/pages/analysis/packets/analysisTableInListContent.ftl</result>
		</action>
		
		<!-- 패킷분석 삭제 -->
		<action name="delPktDumps" method="delPktDumps" class="faultAnalysisAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 패킷분석 시작 -->
		<action name="startPktdump" method="startPktdump" class="faultAnalysisAction">
			<result type="jsonHierarchy"/>
		</action>	
		
		<!-- 패킷분석 중지 -->
		<action name="stopPktdump" method="stopPktdump" class="faultAnalysisAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 패킷분석 취소 -->
		<action name="cancelPktdump" method="cancelPktdump" class="faultAnalysisAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 패킷 수집 Data Status -->
		<action name="loadPktDumpStatusListContent" method="loadPktDumpStatusListContent" class="faultAnalysisAction">	
			<result type="freemarker">/WEB-INF/pages/analysis/packets/pktDumpStatusTableInListContent.ftl</result>
		</action>
		
		<!-- 패킷 수집 Chart Status -->
		<action name="loadPktDumpChartStatusListContent" method="loadPktDumpChartStatusListContent" class="faultAnalysisAction">	
			<result type="freemarker">/WEB-INF/pages/analysis/packets/pktDumpChartStatusTableInListContent.ftl</result>
		</action>
				
		<!-- 패킷 수집 CPU, MEMORY -->
		<action name="loadCpuChartInfo" method="loadCpuChartInfo" class="faultAnalysisAction">
			<result type="jsonHierarchy"/>
		</action>
		
		<!-- 패킷 덤프 파일 유무 검사 -->
		<action name="checkDownloadPktDumpDataExist" method="checkDownloadPktDumpDataExist" class="faultAnalysisAction">
			<result type="json">
				<param name="ignoreHierarchy">false</param>
			</result>
		</action>
		
		<!--  패킷 덤프 다운로드 -->
		<action name="downloadPktDump" method="downloadPktDump" class="faultAnalysisAction">
        	<result type="stream">
                <param name="contentType">application/octet-stream</param>
                <param name="contentLength">${contentLength}</param>
                <param name="contentDisposition">${contentDisposition}</param>
                <param name="inputName">inputStream</param>
                <param name="bufferSize">8192</param>
            </result>
        </action>
	</package>
</struts>