<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>		
	<package name="respTime" namespace="/respTime" extends="json-hierarchy">
		<action name="loadResponseTimeContent" method="loadResponseTimeContent" class="respTimeAction">
			<result type="freemarker">/WEB-INF/pages/sysTools/responseTime/responseTimeContent.ftl</result>
		</action>	
		<action name="loadResponseInfoList" method="loadResponseInfoList" class="respTimeAction">
			<result type="freemarker">/WEB-INF/pages/sysTools/responseTime/responseTimeList.ftl</result>
		</action>
		<action name="loadResponseTimeHistoryInfo" method="loadResponseTimeHistoryInfo" class="respTimeAction">
			<result type="jsonHierarchy"/>
		</action>
		<action name="loadRespInfoAddContent" method="loadRespInfoModifyContent" class="respTimeAction">
			<result type="freemarker">/WEB-INF/pages/sysTools/responseTime/responseTimeAddPopupContent.ftl</result>	
		</action>
		<action name="loadRespInfoModifyContent" method="loadRespInfoModifyContent" class="respTimeAction">
			<result type="freemarker">/WEB-INF/pages/sysTools/responseTime/responseTimePopupContent.ftl</result>	
		</action>
		<action name="addRespIntervalCheck" method="addRespIntervalCheck" class="respTimeAction">
			<result type="jsonHierarchy"/>
		</action>
		<action name="setRespIntervalCheck" method="setRespIntervalCheck" class="respTimeAction">
			<result type="jsonHierarchy"/>
		</action>
		<action name="delRespIntervalCheck" method="delRespIntervalCheck" class="respTimeAction">
			<result type="freemarker">/WEB-INF/pages/sysTools/responseTime/responseTimeContent.ftl</result>
		</action>
		<action name="delRespIntervalCheck" method="delRespIntervalCheck" class="respTimeAction">
			<result type="jsonHierarchy"/>
		</action>
	</package>
</struts>